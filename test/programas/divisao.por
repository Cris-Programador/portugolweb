programa
{
	funcao inicio()
	{
		// Testando divisão inteira
		inteiro a = 10
		para(inteiro i = 1; i <= a; i++)
			escreva(a,"/",i,": ",a / i,"\n")

		// Testando divisão real
		real aa = 10.0
		para(inteiro i = 1; i <= aa; i++)
			escreva(aa,"/",i,": ",aa / i,"\n")
	}
}
