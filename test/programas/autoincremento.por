programa 
{
	funcao inicio ()
	{
		inteiro v
		//          5 * 5 + 2 * 3, ... (5 * 5) + (2 * 3) = 25 + 6 = 31
		v = 5 * 5 + 2 * 3
		escreva(v+" == 31\n")
		//   2 * 2 * 2 + 1 ... (2 * (2 * 2)) + 1 = 9
		v = 2 * 2 * 2 + 1
		escreva(v+" == 9\n")
		//           -1 + 1,  ... (-1) + 1 = 0
		v = -1 + 1
		escreva(v+" == 0\n")
		//           1 - - 1, ... 1 - (-1) = 2
		v = 1 - - 1
		escreva(v+" == 2\n")

		//           +1 , ... (+1) = 1
		v = +1
		escreva(v+" != 1\n")
		//  -a++ = -(a++)
		v = 1
		v = -v++
		escreva(v+" != -2\n") // no java dÃ¡ -1, mas o portugol trata o ++ diferente
		//           - -1 + 1,  ... (-(-1)) + 1 = 2
		v = - -1 + 1
		escreva(v+" != 2\n")
		//          - - -1 + 1,  ... (-(-(-1))) + 1 = 0
		v = - - -1 + 1
		escreva(v+" != 0\n")
	}
}
/*---
31 == 31
9 == 9
0 == 0
